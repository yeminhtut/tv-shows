{
  "name": "hooq",
  "version": "1.0.0",
  "description": "TV shows Demo",
  "license": "MIT",
  "repository": "https://github.com/yeminhtut/tv-shows",
  "scripts": {
    "build": "BUILD_ENV=development NODE_ENV=production webpack --config webpack.prod",
    "format": "prettier --write \"{src,config}/**/*{.js,.jsx}\"",
    "lint": "eslint --ext .jsx --ext .js src config",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress --colors --port 8080 --config webpack.dev",
    "start": "npm run dev",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch --updateSnapshot"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.less": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/tests"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>src/tests/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.6.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.2",
    "chalk": "^2.0.1",
    "css-loader": "^3.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.0",
    "webpack-jarvis": "^0.3.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@svgr/webpack": "^4.3.3",
    "add": "^2.0.6",
    "antd": "^3.19.8",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "normalize.css": "^8.0.1",
    "papaparse": "^5.1.0",
    "path-to-regexp": "^3.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.11",
    "react-modal": "^3.11.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.0.1",
    "react-router-last-location": "^2.0.1",
    "react-sortable-hoc": "^0.8.3",
    "redux": "^4.0.4",
    "redux-form": "^7.2.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.3",
    "yarn": "^1.22.0"
  }
}
